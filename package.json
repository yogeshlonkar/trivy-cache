{
  "name": "trivy-cache-action",
  "version": "0.1.1",
  "private": true,
  "description": "Cache .trivy directory using trivy db version sha",
  "main": "dist/restore/index.js",
  "scripts": {
    "all": "npm run lint && npm run format && npm run test && npm run build",
    "build": "tsc && ncc build -o dist/restore src/restore.ts && ncc build -o dist/save src/save.ts",
    "test": "tsc --noEmit && jest --coverage",
    "lint": "eslint **/*.ts --cache --fix",
    "lint-check": "eslint **/*.ts --cache",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yogeshlonkar/trivy-cache-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "trivy",
    "trivy-db",
    "cache"
  ],
  "author": "yogeshlonkar",
  "license": "MIT",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16 <17"
  },
  "dependencies": {
    "@actions/cache": "^3.2.2",
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/http-client": "^2.1.1",
    "@actions/io": "^1.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/nock": "^11.1.0",
    "@types/node": "^16.18.54",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "nock": "^13.4.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
